{
    "name": "haproxy-discovery",
    "haproxy" : {
        "ec2" : {
            "query_tags" : {
                <% if node['haproxy']['ec2']['query_tags'] -%>
                  <% node['haproxy']['ec2']['query_tags'].each do |tagName,tagValue| -%>
                    "<%= tagName %>" : "<%= tagValue %>"
                  <% end -%>
                <% end -%>
            }
        }
    },
    "run_list": ["alfresco::haproxy-ec2-discovery"]
}
