{
    "name": "haproxy-discovery",
    "tomcat" : {
      "jvm_route" : "<%= node['tomcat']['jvm_route'] %>"
    },
    "haproxy" : {
      "ec2" : {
        "discovery_enabled" : true
      }
    },
    "commons" : {
        "ec2_discovery" : <%= node['commons']['ec2_discovery'].to_json %>
    },
    "run_list": ["alfresco::haproxy-config"]
}
